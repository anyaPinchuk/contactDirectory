<html xmlns:jsp="http://java.sun.com/JSP/Page"
     xmlns:c="http://java.sun.com/jsp/jstl/core">
<link href="${pageContext.request.contextPath}/web-resources/css/index.css" rel="stylesheet" type="text/css" media="screen"/>
<jsp:directive.page contentType="text/html" pageEncoding="UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <body>
    <table>
    <tbody>
    <tr><th>ID</th><th>Name</th></tr>
    <c:forEach items="${requestScope.contactList}" var="contact">
        <tr><td><c:out value="${contact.id}"></c:out> </td>
            <td><c:out value="${contact.name}"></c:out> <br/>
                <c:out value="${contact.surname}"></c:out>
                <c:out value="${contact.thirdName}"></c:out></td>
            </tr>
    </c:forEach>

    </tbody>
    </table>
</body>
</html>
