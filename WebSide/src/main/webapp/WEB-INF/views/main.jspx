<html xmlns:jsp="http://java.sun.com/JSP/Page"
     xmlns:c="http://java.sun.com/jsp/jstl/core">
<link href="${pageContext.request.contextPath}/web-resources/css/index.css" rel="stylesheet" type="text/css" media="screen"/>
<link href="${pageContext.request.contextPath}/web-resources/bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css" media="screen"/>
<jsp:directive.page contentType="text/html" pageEncoding="UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <body>
    <h1>Справочник контактов</h1>
    <table class="table-bordered table">
    <tbody>
    <tr><th>ID</th><th>FIO</th><th>Date of Birth</th><th>Address</th><th>Company</th></tr>
    <c:forEach items="${requestScope.contactList}" var="contact">
        <tr><td><c:out value="${contact.id}"/> </td>
            <td><a><c:out value="${contact.name}"/><br/>
                <c:out value="${contact.surname}"/><br/>
                <c:out value="${contact.thirdName}"/></a></td>
            <td><c:out value="${contact.dateOfBirth}"/></td>
            <!--<td><c:if test="${contact.address}!=null">-->
            <!--<c:out value="${contact.address.country}"/>, <c:out value="${contact.address.city}"/>-->
            <!--<c:out value="${contact.address.streetAddress}"/>, <c:out value="${contact.address.index}"/>-->
        <!--</c:if></td>-->
        <td><c:out value="${contact.address.country}"/>, <c:out value="${contact.address.city}"/>,
                <c:out value="${contact.address.streetAddress}"/>, <c:out value="${contact.address.index}"/></td>
            <td><c:out value="${contact.job}"/></td>
            </tr>
    </c:forEach>

    </tbody>
    </table>
</body>
</html>
